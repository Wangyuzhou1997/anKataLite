<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BannerView">
        <attr name="dot_size" format="dimension" />
        <attr name="dot_distance" format="dimension" />
        <attr name="dot_normal" format="color|reference" />
        <attr name="dot_focus" format="color|reference" />
        <attr name="gravity" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="-1" />
        </attr>
        <attr name="indicator_bg" format="color" />

    </declare-styleable>
</resources>